<!-- Create your form here -->
<h2>New Student Form</h2>

<%= form_tag students_path do %>

  <label>First Name:</label>
  <%= text_field_tag :first_name, nil, id:'first_name' %> <br>

  <label> Last Name: </label>
  <%= text_field_tag :last_name, nil, id: 'last_name' %> <br>

  <%= submit_tag "Submit Student" %>

<% end %>
